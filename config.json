{
    "Dataset":{
        "DynamicDataset":{
            "path_images":"./example_images",
            "ext":".png"
        },
        "StaticDataset":{
            "path_joints":"./example_keypoints",
            "ext":".json",
            "im_width":1920,
            "im_height":1080
        },
        "Transforms":{
            "normalize":true,
            "body_parts":true,
            "translation":{
                "enable":true,
                "p":0.9,
                "distance":40
            }
        }
    },
    "Model":{
        "PosTER":{
            "dim_tokens": 3,
            "dim_embed" : 100,  
            "dim_ff" : 2048,
            "nlayers" : 4,  
            "nhead" : 5,  
            "dropout" : 0.1 
        }
    }
}